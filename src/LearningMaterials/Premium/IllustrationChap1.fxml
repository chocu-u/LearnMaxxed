                     <image>
                        <Image url="@../../../Resources/thumbnail.jpg" />
                     </image>
                  </ImageView>
                  <Text layoutX="55.0" layoutY="2185.0" strokeType="OUTSIDE" strokeWidth="0.0" text="• Gesture Drawing – Quick drawings that capture the movement and posture of a subject." textAlignment="JUSTIFY" wrappingWidth="727.7760620117188">
                     <font>
                        <Font name="Segoe UI" size="16.0" />
                     </font>
                  </Text>
                  <Text layoutX="84.0" layoutY="2215.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Example: Drawing a person in motion using fast, loose strokes." textAlignment="JUSTIFY" wrappingWidth="713.7760620117188">
                     <font>
                        <Font name="Segoe UI" size="16.0" />
                     </font>
                  </Text>
                  <ImageView fitHeight="150.0" fitWidth="267.0" layoutX="264.0" layoutY="2229.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../Resources/gesture.jpg" />
                     </image>
                  </ImageView>
                  <Text layoutX="55.0" layoutY="2398.0" strokeType="OUTSIDE" strokeWidth="0.0" text="• Contour Drawing – Drawing the outline of an object without lifting the pencil." textAlignment="JUSTIFY" wrappingWidth="727.7760620117188">
                     <font>
                        <Font name="Segoe UI" size="16.0" />
                     </font>
                  </Text>
                  <Text layoutX="84.0" layoutY="2427.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Example: Slowly tracing the edges of a face to improve observation." textAlignment="JUSTIFY" wrappingWidth="713.7760620117188">
                     <font>
                        <Font name="Segoe UI" size="16.0" />
