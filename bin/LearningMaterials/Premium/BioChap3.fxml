                  <JFXButton fx:id="premnextButton" buttonType="RAISED" layoutX="573.0" layoutY="1850.0" onAction="#finishButtonHandler" prefHeight="38.0" prefWidth="202.0" style="-fx-text-fill: white; -fx-background-radius: 25; -fx-background-color: #42736c;" text="Finish" textFill="#42736c">
